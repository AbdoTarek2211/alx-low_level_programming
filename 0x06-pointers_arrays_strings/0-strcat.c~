#include "main.h"
/**
 * _strcat - appends the src string to the dest string
 * @dest : string of the destination
 * @src : string to be put at
 *
 * Return: 0 or 1
 */
char *_strcat(char *dest, char *src)
{
int x;
int y;
for (x = 0; dest[x] != '\0'; ++x);
for (y = 0; src[y] != '\0'; ++y; ++x)
{
dest[x] = src[y];
}
dest[x] = '\0';
return (dest);
}
